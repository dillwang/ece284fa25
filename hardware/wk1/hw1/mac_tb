    %vpi_call/w 2 144 "$display", "-------------------- Computation completed For Sign and Magnitude Mode--------------------" {0 0 0};
    %delay 10, 0;
    %vpi_call/w 2 147 "$finish" {0 0 0};
    %end;
    .thread T_4;
